package nomor1;
import java.util.ArrayList;
import java.util.List;
import java.util.Scanner;
/**
 *
 * @author Praktikum
 */
public class Nomor1 {

    /**
     * @param args the command line arguments
     */      
    
    private int pas=0;
    private int curen=1;
    private int fibonanci;
    private List<Integer> list=new ArrayList<Integer>();
    
    public Nomor1(){
        list.add(1);
        list.add(2);
    }
    
    public List<Integer> getFibonacci(int limit){
        
        for(int i=0;i<limit-2;i++){
            fibonanci=curen+pas;
            pas=curen;
            curen=fibonanci;
            
            list.add(fibonanci);
        }
        
        return list;
    }
}

   public static void main( String[] args ) {
   
        ibonanciFibonanci fibonanci=new Fibonacci();
        List<Integer> list=fibonacci.getFibonacci(10);
        
        for(Integer i:list){
            System.out.print(i+", ");
        }
       
    

       
        
        
    
    
